{
  "name": "dispatch",
  "license": "UNLICENSED",
  "scripts": {
    "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
    "report": "shadow-cljs run shadow.cljs.build-report ui target/build-report.html",
    "assets": "capacitor-assets generate --iconBackgroundColor \"#171717\" --splashBackgroundColor \"#171717\"",
    "db-gen": "prisma generate",
    "db-push": "prisma db push --accept-data-loss",
    "db-reset": "prisma migrate reset --force",
    "watch": "shadow-cljs watch api ui",
    "serve": "nodemon --watch out out/api.js -e js",
    "test": "yarn db-reset && yarn db-push && shadow-cljs release test && node out/test.js",
    "migrate": "yarn db-reset && yarn db-push && shadow-cljs release test && node out/test.js",
    "release": "shadow-cljs release api ui --config-merge ./config.edn && ncc build out/api.js -o dist/ --minify",
    "release:api": "shadow-cljs release api --config-merge ./config.edn && ncc build out/api.js -o dist/ --minify",
    "release:ui": "shadow-cljs release ui --config-merge ./config.edn",
    "cap:dev": "export API_URL=https://api.dispatch.ambito.dev && npx cap sync",
    "cap:android": "export RELOAD_URL=http://10.0.2.2:8000 && npx cap sync"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@apollo/server": "^4.0.4",
    "@capacitor-community/background-geolocation": "^1.2.7",
    "@capacitor/android": "^4.6.2",
    "@capacitor/core": "^4.6.2",
    "@capacitor/device": "^4.1.0",
    "@capacitor/ios": "^4.6.2",
    "@capgo/capacitor-updater": "^4.17.11",
    "@floating-ui/react-dom": "^1.0.1",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@headlessui/react": "^1.6.6",
    "@prisma/client": "^4.8.0",
    "apollo-link-scalars": "^4.0.1",
    "aws-rum-web": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "framer-motion": "^7.6.2",
    "googleapis": "^109.0.1",
    "graphql": "^16.6.0",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.4.2",
    "serverless-http": "^3.1.0",
    "use-lilius": "^2.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^4.6.2",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@vercel/ncc": "^0.34.0",
    "cross-fetch": "^3.1.5",
    "global-jsdom": "^8.6.0",
    "jsdom": "^20.0.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.8.0",
    "shadow-cljs": "2.19.9",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.9.4"
  }
}
