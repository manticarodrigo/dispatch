{
  "name": "dispatch",
  "license": "UNLICENSED",
  "scripts": {
    "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
    "report": "shadow-cljs run shadow.cljs.build-report ui target/build-report.html",
    "db-push": "prisma db push",
    "watch": "shadow-cljs watch api ui --config-merge ./config/dev.edn",
    "serve": "nodemon --watch out out/api.js -e js",
    "test": "prisma migrate reset --force && prisma db push && shadow-cljs compile test && node out/test.js",
    "release": "shadow-cljs release api ui --config-merge ./config/prod.edn && ncc build out/api.js -o dist/ --minify"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@apollo/server": "^4.0.4",
    "@capacitor/core": "^4.0.1",
    "@capacitor/geolocation": "^4.0.1",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@headlessui/react": "^1.6.6",
    "@prisma/client": "^4.5.0",
    "axios": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^7.6.2",
    "graphql": "^16.6.0",
    "highlight.js": "11.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.4.2",
    "serverless-http": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vercel/ncc": "^0.34.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "global-jsdom": "^8.6.0",
    "jsdom": "^20.0.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "shadow-cljs": "2.19.9",
    "tailwindcss": "^3.1.8"
  }
}
