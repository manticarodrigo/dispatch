    "@visx/scale": "^3.0.0",
    "@visx/shape": "^3.0.0",
    "@visx/tooltip": "^3.1.2",
    "apollo-link-scalars": "^4.0.1",
    "aws-rum-web": "^1.12.0",
    "aws-sdk": "^2.1312.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "d3-array": "^3.2.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
